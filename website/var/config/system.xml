<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
  <general>
    <timezone>Europe/Berlin</timezone>
    <php_cli></php_cli>
    <domain></domain>
    <language>de</language>
    <validLanguages>en_GB,de_AT</validLanguages>
    <theme>/pimcore/static/js/lib/ext/resources/css/xtheme-gray.css</theme>
    <loginscreenimageservice>1</loginscreenimageservice>
    <loginscreencustomimage></loginscreencustomimage>
    <debug>1</debug>
    <debug_ip>62.99.163.106</debug_ip>
    <firephp></firephp>
    <loglevel>
      <debug></debug>
      <info>1</info>
      <notice>1</notice>
      <warning>1</warning>
      <error></error>
      <critical>1</critical>
      <alert>1</alert>
      <emergency>1</emergency>
    </loglevel>
    <devmode></devmode>
    <logrecipient></logrecipient>
    <viewSuffix></viewSuffix>
  </general>
  <database>
    <adapter>Pdo_Mysql</adapter>
    <params>
      <host>127.0.0.1</host>
      <username>dev4</username>
      <password>dev4</password>
      <dbname>pimcore_dev4</dbname>
      <port>3306</port>
    </params>
  </database>
  <documents>
    <versions>
      <days></days>
      <steps>20</steps>
    </versions>
    <default_controller>default</default_controller>
    <default_action>default</default_action>
    <error_pages>
      <default>/de</default>
    </error_pages>
    <allowtrailingslash></allowtrailingslash>
    <allowcapitals></allowcapitals>
  </documents>
  <objects>
    <versions>
      <days></days>
      <steps>20</steps>
    </versions>
  </objects>
  <assets>
    <webdav>
      <hostname></hostname>
    </webdav>
    <versions>
      <days></days>
      <steps>20</steps>
    </versions>
    <ffmpeg></ffmpeg>
    <qtfaststart></qtfaststart>
  </assets>
  <services>
    <googlemaps>
      <apikey></apikey>
    </googlemaps>
    <translate>
      <apikey></apikey>
    </translate>
    <google>
      <username></username>
      <password></password>
    </google>
  </services>
  <cache>
    <enabled></enabled>
    <lifetime></lifetime>
    <excludePatterns></excludePatterns>
    <excludeCookie>pimcore_admin_sid</excludeCookie>
  </cache>
  <outputfilters>
    <imagedatauri></imagedatauri>
    <less></less>
    <lesscpath></lesscpath>
    <cssminify></cssminify>
    <javascriptminify></javascriptminify>
    <javascriptminifyalgorithm></javascriptminifyalgorithm>
  </outputfilters>
  <email>
    <sender>
      <name></name>
      <email></email>
    </sender>
    <return>
      <name></name>
      <email></email>
    </return>
    <method></method>
    <smtp>
      <host></host>
      <port></port>
      <ssl></ssl>
      <name></name>
      <auth>
        <method></method>
        <username></username>
        <password></password>
      </auth>
    </smtp>
    <debug>
      <emailaddresses></emailaddresses>
    </debug>
  </email>
  <webservice>
    <enabled></enabled>
  </webservice>
  <httpclient>
    <adapter>Zend_Http_Client_Adapter_Socket</adapter>
    <proxy_host></proxy_host>
    <proxy_port></proxy_port>
    <proxy_user></proxy_user>
    <proxy_pass></proxy_pass>
  </httpclient>
</zend-config>
