<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
  <general>
    <timezone>Europe/Vienna</timezone>
    <php_cli>/usr/local/zend/bin/php</php_cli>
    <domain>dev4.pimcore.org</domain>
    <redirect_to_maindomain></redirect_to_maindomain>
    <language>de</language>
    <validLanguages>en_GB,de_AT</validLanguages>
    <theme></theme>
    <loginscreenimageservice>1</loginscreenimageservice>
    <loginscreencustomimage></loginscreencustomimage>
    <debug></debug>
    <debug_ip></debug_ip>
    <http_auth>
      <username></username>
      <password></password>
    </http_auth>
    <firephp></firephp>
    <loglevel>
      <debug></debug>
      <info></info>
      <notice></notice>
      <warning></warning>
      <error></error>
      <critical>1</critical>
      <alert>1</alert>
      <emergency>1</emergency>
    </loglevel>
    <debug_admin_translations></debug_admin_translations>
    <devmode></devmode>
    <logrecipient></logrecipient>
    <viewSuffix></viewSuffix>
    <targeting>1</targeting>
  </general>
  <database>
    <adapter>Pdo_Mysql</adapter>
    <params>
      <host>127.0.0.1</host>
      <username>dev4</username>
      <password>dev4</password>
      <dbname>pimcore_dev4</dbname>
      <port>3306</port>
    </params>
  </database>
  <documents>
    <versions>
      <days></days>
      <steps>0</steps>
    </versions>
    <default_controller>default</default_controller>
    <default_action>default</default_action>
    <error_pages>
      <default>/</default>
    </error_pages>
    <createredirectwhenmoved></createredirectwhenmoved>
    <allowtrailingslash></allowtrailingslash>
    <allowcapitals></allowcapitals>
  </documents>
  <objects>
    <versions>
      <days></days>
      <steps>0</steps>
    </versions>
  </objects>
  <assets>
    <webdav>
      <hostname></hostname>
    </webdav>
    <versions>
      <days></days>
      <steps>0</steps>
    </versions>
    <ffmpeg></ffmpeg>
    <qtfaststart></qtfaststart>
    <icc_rgb_profile></icc_rgb_profile>
    <icc_cmyk_profile></icc_cmyk_profile>
  </assets>
  <services>
    <translate>
      <apikey>AIzaSyBbtPCzVQLBZk2Hsi3ysP6Egs6D-b93H0A</apikey>
    </translate>
    <google>
      <client_id></client_id>
      <email></email>
      <simpleapikey>AIzaSyBbtPCzVQLBZk2Hsi3ysP6Egs6D-b93H0A</simpleapikey>
      <browserapikey></browserapikey>
    </google>
  </services>
  <cache>
    <enabled></enabled>
    <lifetime></lifetime>
    <excludePatterns></excludePatterns>
    <excludeCookie>pimcore_admin_sid</excludeCookie>
  </cache>
  <outputfilters>
    <imagedatauri></imagedatauri>
    <less>1</less>
    <lesscpath></lesscpath>
    <cssminify></cssminify>
    <javascriptminify></javascriptminify>
    <javascriptminifyalgorithm>jsmin</javascriptminifyalgorithm>
  </outputfilters>
  <email>
    <sender>
      <name>Pimcore Demo - Nikolaus Flamann</name>
      <email>nikolaus@flamann.at</email>
    </sender>
    <return>
      <name>Pimcore Demo - Nikolaus Flamann</name>
      <email>nikolaus@flamann.at</email>
    </return>
    <method>sendmail</method>
    <smtp>
      <host></host>
      <port></port>
      <ssl></ssl>
      <name></name>
      <auth>
        <method></method>
        <username></username>
        <password></password>
      </auth>
    </smtp>
    <debug>
      <emailaddresses>nikolaus@flamann.at</emailaddresses>
    </debug>
  </email>
  <webservice>
    <enabled></enabled>
  </webservice>
  <httpclient>
    <adapter>Zend_Http_Client_Adapter_Socket</adapter>
    <proxy_host></proxy_host>
    <proxy_port></proxy_port>
    <proxy_user></proxy_user>
    <proxy_pass></proxy_pass>
  </httpclient>
</zend-config>
